{
  "name": "husky-conf",
  "version": "1.0.0",
  "description": "Configure husky in your project.",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint src/**/*.js",
    "pretest": "./node_modules/.bin/eslint --fix --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "bin": {
    "husky-conf": "index.js"
  },
  "files": [
    "index.js",
    "husky-conf.js"
  ],
  "keywords": [
    "cli",
    "husky",
    "config",
    "husky-config",
    "husky-conf",
    "git-hooks",
    "hooks"
  ],
  "author": "Joydip Roy (rjoydip)",
  "email": "joydipand@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^5.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "husky": "^0.14.3",
    "jest": "^23.1.0"
  },
  "jest": {
    "transform": {}
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "dot-prop": "^4.2.0",
    "lodash": "^4.17.10",
    "meow": "^5.0.0",
    "read-pkg": "^3.0.0",
    "update-notifier": "^2.5.0",
    "write-json-file": "^2.3.0",
    "write-pkg": "^3.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  }
}
